}

//MEMPROSES ARRAY 1
int panjang = apa.size();
for (int i = 0; i < panjang; i++) {
   int dur = apa.get(i).getDuration();
   String jdl = apa.get(i).getTitle();
   System.out.printf("%-20s %15s %n",jdl,dur);
}

